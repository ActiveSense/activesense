<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="ActiveSense.Desktop.App"
             xmlns:local="using:ActiveSense.Desktop"
             RequestedThemeVariant="Default"
             xmlns:semi="https://irihi.tech/semi">

    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <semi:SemiTheme />
        <!-- <style:FluentAvaloniaTheme></style:FluentAvaloniaTheme> -->
        <StyleInclude Source="Assets/Icons.axaml"></StyleInclude>
        <StyleInclude Source="Assets/Styles.axaml"></StyleInclude>
    </Application.Styles>
</Application>